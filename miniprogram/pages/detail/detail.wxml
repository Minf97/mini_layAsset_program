<view class="container">
<view class="textBox">
  <van-cell-group inset>
    <block wx:for="{{textDetail}}" wx:key="*this">
      <van-cell title="{{item.label}}" value="{{item.value}}" label="{{item.des}}" border="{{ false }}" />
    </block>
  </van-cell-group>
</view>
  
  <!-- 九宫格 -->
  <van-grid column-num="3" border="{{ false }}">
    <van-grid-item use-slot wx:for="{{ 3 }}" wx:for-item="index" wx:key="*this">
      <image style="width: 100%; height: 90px;" src="https://img.yzcdn.cn/vant/apple-{{ index + 1 }}.jpg" />
    </van-grid-item>
  </van-grid>

</view>